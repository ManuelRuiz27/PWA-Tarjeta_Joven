{
  "name": "tj-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node ./scripts/postbuild.js",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint ./src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --runInBand",
    "db:seed": "node ./scripts/run-ts.js scripts/seed.ts",
    "seed:test-users": "node ./scripts/run-ts.js scripts/create-test-users.ts",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^7.7.3",
    "@fastify/rate-limit": "^8.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.19.1",
    "@sentry/node": "^7.118.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.19.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.0.0",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/parser": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.2",
    "@types/supertest": "^6.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": "^20.16.0",
    "npm": ">=10.0.0"
  }
}
